{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "MeetlyOmniDb": "Host=localhost;Database=${DB_NAME};Username=${DB_USER};Password=${DB_PASS}"
  },
  "Jwt": {
    "Issuer": "MeetlyOmni-API",
    "Audience": "localhost3000",
    "AccessTokenMinutes": 15
  }
}
