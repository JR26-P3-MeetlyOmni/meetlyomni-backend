{
  "family": "meetly-omni-backend-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::351889159066:role/meetly-omni-backend-ecs-task-execution",
  "containerDefinitions": [
    {
      "name": "meetly-omni-backend",
      "image": "351889159066.dkr.ecr.ap-southeast-2.amazonaws.com/meetly-omni-backend-dev:latest",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "ASPNETCORE_ENVIRONMENT",
          "value": "UAT"
        },
        {
          "name": "ASPNETCORE_URLS",
          "value": "http://0.0.0.0:80"
        }
        ],
      "secrets": [
        {
          "name": "ConnectionStrings__MeetlyOmniDb",
          "valueFrom": "arn:aws:ssm:ap-southeast-2:351889159066:parameter/ConnectionStrings__MeetlyOmniDb"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/meetly-omni-backend-task",
          "awslogs-region": "ap-southeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
    "operatingSystemFamily": "LINUX"
  }
}
